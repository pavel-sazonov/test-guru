<p>
  <strong><%= t('.title') %>:</strong>
  <%= @test.title %>
</p>

<p>
  <strong><%= t('.level') %>:</strong>
  <%= @test.level %>
</p>

<h2><%= t('.questions') %>:</h2>

<p>
  <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
</p>

<table>
  <tr>
    <th><%= t('.body') %></th>
    <th colspan="3"><%= t('.action') %></th>
  </tr>
  <%= render @test.questions %></table>

<p>
  <%= link_to t('.edit'), edit_admin_test_path(@test) %>
  <%= link_to t('.back'), admin_tests_path %>
</p>
